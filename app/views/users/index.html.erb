<h4>Users</h4>
<table id="users" class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Name</th>
      <th scope="col">E-mail</th>
      <th scope="col">Role</th>
      <th scope="col">Active</th>
      <th scope="col">Last Signed In</th>
      <th scope="col">&nbsp;</th>
    </tr>
  </thead>
  <tbody id="users">
    <% @users.each do |user| %>
      <%= render user %>
    <% end %>
  </table>
</div>
