<h4>Artworks</h4>
<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Artist</th>
      <th scope="col">Title</th>
      <th scope="col">Medium</th>
      <th scope="col">Description</th>
      <th scope="col">&nbsp;</th>
    </tr>
  </thead>
  <tbody id="artworks">
    <% @artworks.each do |artwork| %>
      <%= render artwork %>
    <% end %>
  </table>
</div>
